{
  "consumer": {
    "name": "FrontendWebsite"
  },
  "provider": {
    "name": "EndpointService"
  },
  "interactions": [
    {
      "description": "api request",
      "providerState": "endpoint exist",
      "request": {
        "method": "GET",
        "path": "/path",
        "headers": {
          "Authorization": "Bearer 2019-01-14T11:34:18.045Z"
        },
        "matchingRules": {
          "$.headers.Authorization": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "id": "1234",
            "name": "example"
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}